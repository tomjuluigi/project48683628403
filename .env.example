# Application
NODE_ENV=development
PORT=5000

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Supabase
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Privy Authentication
VITE_PRIVY_APP_ID=your-privy-app-id
PRIVY_APP_SECRET=your-privy-app-secret

# Session
SESSION_SECRET=your-session-secret-key

# ===== REGISTRY CONTRACT CONFIGURATION =====
# On-chain registry for verifiable platform activity (for grants, analytics, etc.)

# Registry Contract Address (set after deployment)
REGISTRY_CONTRACT_ADDRESS=

# Platform wallet for registering coins on-chain
PLATFORM_WALLET_PRIVATE_KEY=      # KEEP SECRET! Never commit this
PLATFORM_WALLET_ADDRESS=          # Public address of platform wallet

# Deployment wallet (can be same as platform wallet)
DEPLOYER_PRIVATE_KEY=             # For contract deployment

# Network RPC URLs
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# BaseScan API Key (for contract verification)
# Get free key from: https://basescan.org/apis
BASESCAN_API_KEY=

# Registry Cron Schedule (optional, default: every 6 hours)
# Format: cron schedule string
# Examples:
#   0 */6 * * *   - Every 6 hours
#   0 */12 * * *  - Every 12 hours
#   0 0 * * *     - Daily at midnight
REGISTRY_CRON_SCHEDULE=0 */6 * * *

# Telegram Bot (optional)
TELEGRAM_BOT_TOKEN=
